PROGRAMA
        ENT x = 5 + 2
        ENT y = 3

        IMPRIME("Valor de x es " + x)
        IMPRIME("Valor de y es " + y)
        IMPRIME("Resultado de x por y " + x*y)

        STR hola = "Hola Mundo"
        IMPRIME(hola)

        CONST DEC PI = 3.141592653
        IMPRIME(PI)

        BOOL isOK = VERDADERO
        IMPRIME("Valor de isOk es " + isOK)

        SI(isOK) ENTONCES
        {
            LEE(x)
            SI(x > 10) ENTONCES
            {
                IMPRIME("El numero es mayor que 10")
            }
            SINO
            {
                IMPRIME("El numero es menor que 10")
            }
        }

        STR nombre
        IMPRIME("Introduzca su nombre")
        LEE(nombre)
        IMPRIME(nombre)

        IMPRIME("Numeros del 1 al 10")
        ENT it = 1
        MIENTRAS (it <= 10)
        {
            IMPRIME(it)
            it = it + 1
        }
FIN